# RILM needs the country code in all articles. This information must be taken from the superordinate work (collection).

# select Id of all collections and assign the country code

select pica_match('099@b','sb');

if pica_match('099@b','sb')
   pica_map('019@a','country',join:', ')
   pica_map('003@0',"_id")
end

remove_field('record');



